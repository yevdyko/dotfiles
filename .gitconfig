[user]
  email = yevdyko@gmail.com
  name = Yev Dyko
[core]
  autocrlf = false
  safecrlf = false
  editor = vim
  excludesfile = /Users/yevdyko/.gitignore_global
  savecrlf = true
[push]
  default = matching
[alias]
  del = branch -d
  br = branch
  com = checkout master
  ch = checkout
  gr = grep -Ii
  am = commit --amend
  amend = commit --amend -m
  f = fetch
  po = push origin
  pom = push origin master
  poh = push origin HEAD
  pl = pull
  plom = pull origin master
  rv = remote -v
  s = status --short --branch
  d = diff
  aa = add -A
  c = commit
  cm = commit -m
  hist = "log --pretty=format:'%h %ad | %s%d [%an]' --graph --date=short"
  type = cat-file -t
  dump = cat-file -p
[color]
  ui = true
[filter "lfs"]
  clean = git-lfs clean -- %f
  smudge = git-lfs smudge -- %f
  process = git-lfs filter-process
  required = true
